  <businessRules>
    <rule id="r101" commandName="New" type="Sql" phase="Execute"><![CDATA[SELECT
       @D1 = D1 ,
       @D2 = D2 ,
       @D3 = D3 ,
       @D4 = D4 ,
       @D5 = D5
       FROM [dbo].[Get_Bin_Locations] ( @BusinessRules_UserId )
SET @StoreID=@Session_StoreID
]]></rule>
