SET @s_Error = 0;
IF @PMT_APPLY_BAL <> @Amount_Paid
    BEGIN
        SET @s_Error = 1;
    END;
IF @s_Error = 1
    BEGIN
        SET @Result_ShowAlert = 'Payment Has Application you cannot Cancel';
    END;
IF @Parameters_AreYouSure = 1
   AND
   @s_Error = 0
    BEGIN
        EXEC [dbo].[PAYMENT_TO_GL_REVERSE]
             @PaymentID ,
             @BusinessRules_UserId ,
             @Parameters_CRemarks,
             @Parameters_JVDate
    END;]]></data>
