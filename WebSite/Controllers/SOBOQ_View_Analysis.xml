<?xml version="1.0" encoding="utf-8"?>
<dataController name="SOBOQ_View_Analysis" conflictDetection="overwriteChanges" label="^SoBoqVieAna^SOBOQ_View_Analysis^SoBoqVieAna^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands />
  <fields>
    <field name="SO_NO" type="Decimal" allowNulls="false" label="^BOQId^BOQ #^BOQId^" />
    <field name="Item_Code" type="String" allowNulls="false" label="^IteCod^Item Code^IteCod^" length="20" />
    <field name="Item_Desc" type="String" label="^IteDes^Item Desc^IteDes^" length="500" />
    <field name="UOM" type="String" label="^Uom^Uom^Uom^" length="50" showInSummary="true" />
    <field name="SO_Qty" type="Decimal" label="^SOQty^SO Qty^SOQty^" />
    <field name="BudgetedValues" type="Decimal" label="^BudVal^Budgeted Values^BudVal^" readOnly="true" showInSummary="true" />
    <field name="EstimatedCost" type="Decimal" label="^EstCos^Estimated Cost^EstCos^" readOnly="true" showInSummary="true" />
    <field name="ActualCost" type="Decimal" label="^ActCos^Actual Cost^ActCos^" readOnly="true" showInSummary="true" />
    <field name="Unit_Price" type="Decimal" label="^UnitPrice^Unit Price^UnitPrice^" showInSummary="true" />
    <field name="DV_Balance" type="Decimal" label="^DVBal^DV Balance^DVBal^" showInSummary="true" />
    <field name="SellingValue" type="Decimal" label="^SelVal^Selling Value^SelVal^" readOnly="true" showInSummary="true" />
    <field name="INV_Balance" type="Decimal" label="^INVBal^INV Balance^INVBal^" showInSummary="true" />
    <field name="Vendor_Name" type="String" label="^CusNam^Customer Name^CusNam^" length="250" />
    <field name="YourRefrence" type="String" label="^RefId^Ref.#^RefId^" length="15" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="^SoBoqVieAna^SOBOQ View Analysis^SoBoqVieAna^" reportTemplate="true" sortExpression="SO_NO DESC" showInSelector="false">
      <headerText />
      <dataFields>
        <dataField fieldName="SO_NO" columns="15" />
        <dataField fieldName="Vendor_Name" />
        <dataField fieldName="Item_Code" columns="20" />
        <dataField fieldName="Item_Desc" />
        <dataField fieldName="UOM" columns="50" />
        <dataField fieldName="SO_Qty" dataFormatString="N2" columns="15" />
        <dataField fieldName="BudgetedValues" dataFormatString="N2" columns="15" aggregate="Sum" />
        <dataField fieldName="EstimatedCost" dataFormatString="N2" columns="15" aggregate="Sum" hidden="true" />
        <dataField fieldName="ActualCost" dataFormatString="N2" columns="15" aggregate="Sum" />
        <dataField fieldName="Unit_Price" dataFormatString="N2" columns="15" />
        <dataField fieldName="DV_Balance" dataFormatString="N2" columns="15" />
        <dataField fieldName="SellingValue" dataFormatString="N2" columns="15" />
        <dataField fieldName="INV_Balance" dataFormatString="N2" columns="15" />
        <dataField fieldName="YourRefrence" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="^RevSoBoqAna^Review SOBOQ View Analysis^RevSoBoqAna^" showInSelector="false">
      <headerText />
      <categories>
        <category id="c1" headerText="SOBOQ View Analysis" flow="NewColumn">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="SO_NO" columns="15" />
            <dataField fieldName="Item_Code" columns="20" />
            <dataField fieldName="Item_Desc" />
            <dataField fieldName="UOM" columns="50" />
            <dataField fieldName="SO_Qty" dataFormatString="c" columns="15" />
            <dataField fieldName="BudgetedValues" dataFormatString="c" columns="15" />
            <dataField fieldName="EstimatedCost" dataFormatString="c" columns="15" />
            <dataField fieldName="ActualCost" dataFormatString="c" columns="15" />
            <dataField fieldName="Unit_Price" dataFormatString="c" columns="15" />
            <dataField fieldName="DV_Balance" dataFormatString="c" columns="15" />
            <dataField fieldName="SellingValue" dataFormatString="c" columns="15" />
            <dataField fieldName="INV_Balance" dataFormatString="c" columns="15" />
            <dataField fieldName="Vendor_Name" />
            <dataField fieldName="YourRefrence" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="^NewSoBoqVieAna^New SOBOQ View Analysis^NewSoBoqVieAna^" showInSelector="false">
      <headerText />
      <categories>
        <category id="c1" headerText="New SOBOQ View Analysis" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="SO_NO" columns="15" />
            <dataField fieldName="Item_Code" columns="20" />
            <dataField fieldName="Item_Desc" />
            <dataField fieldName="UOM" columns="50" />
            <dataField fieldName="SO_Qty" dataFormatString="c" columns="15" />
            <dataField fieldName="Unit_Price" dataFormatString="c" columns="15" />
            <dataField fieldName="DV_Balance" dataFormatString="c" columns="15" />
            <dataField fieldName="INV_Balance" dataFormatString="c" columns="15" />
            <dataField fieldName="Vendor_Name" />
            <dataField fieldName="YourRefrence" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
    </actionGroup>
    <actionGroup id="ag2" scope="Form">
      <action id="a1" commandName="Edit" />
      <action id="a3" commandName="Cancel" />
      <action id="a4" whenLastCommandName="Edit" commandName="Update" />
      <action id="a6" whenLastCommandName="Edit" commandName="Cancel" />
