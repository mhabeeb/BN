    <rule id="r102" commandName="New" type="Sql" phase="Execute"><![CDATA[SELECT @Line_Disc_Percent = Header_Disc_Percent,
        @Project_Code      = Project_Code,
        @Dept_Code         = Dept_Code,
        @Transaction_No    = Transaction_No,
