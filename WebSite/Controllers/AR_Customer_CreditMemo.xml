    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[SET @PaymentType='Credit Memo'
SET @APAR_Flag=1
SET @CMDMYN=1
SET @Rec_Status=0

SET @OPU_Code=@Session_OPU_Code
SET @Company_Code=@Session_Company_Code
	  
	 

SET @Cur_Rate = [dbo].[Get_Curr_Rate_v1] (@Currency_Code,@Payment_Date, @Session_OPU_Code)

--SET @Doc_No = dbo.GET_NEXT_DOC_NUMBER (25,@Session_OPU_Code,@Payment_Date)

SET @Doc_No = dbo.GET_NEXT_DOC_NUMBER (@DOC_TYPE_ID,@Session_OPU_Code,@Payment_Date)
EXEC dbo.RESET_SEQUENCE @DOC_TYPE_ID,@Session_OPU_Code,@Payment_Date]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    --SET @Result_ShowViewMessage='Sorry!, You Are ReadOnly User ....You Cannot Change Record'
	SET @Result_Focus = 'Payment_Date,You Are ReadOnly User ....You Cannot Change Record'
end

if [dbo].[Check_Open_Date_SubLedger](@Payment_Date,@BusinessRules_UserId,1)=1
begin
    SET @BusinessRules_PreventDefault = 1
   -- SET @Result_ShowViewMessage='Sorry!, The Transaction Date Is Not In An Open Period'
	SET @Result_Focus = 'Payment_Date,The Transaction Date Is Not In An Open Period'
end

if [dbo].[CHECK_APPROVAL_STATUS](@PaymentID,@DOC_TYPE_ID)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, The Docuement Is InProcess/Approved, Can not be Update'
