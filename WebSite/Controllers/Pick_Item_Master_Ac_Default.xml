  <commands>
    <command id="command1" type="Text" tableName="dbo.tbl_Item_Master" custom="true">
      <text><![CDATA[SELECT        dbo.tbl_Item_Master.Item_Code, dbo.tbl_Item_Master.Part_No, dbo.tbl_Item_Master.Item_Description, dbo.tbl_Item_Master.UOM, 
                         dbo.tbl_Item_Master.Item_Category, dbo.tbl_Item_Master.Bin_Location, dbo.tbl_Item_Master.BranchID, dbo.tbl_Item_Master.IsAssets, 
                         dbo.tbl_Item_Master.Stock_On_Hand, dbo.tbl_Item_Master.IsItemSerialized, dbo.tbl_Item_Master.IsServiceItem, dbo.Default_Accouts.AccountID, 
                         dbo.Default_Accouts.S1, dbo.Default_Accouts.S2, dbo.Default_Accouts.S3, dbo.Default_Accouts.S4, dbo.Default_Accouts.S5, dbo.Default_Accouts.S6, 
                         dbo.Default_Accouts.S7, dbo.Default_Accouts.S8, dbo.Default_Accouts.S9, dbo.Default_Accouts.DefaultID
FROM            dbo.Default_Accouts INNER JOIN
                         dbo.tbl_Item_Master ON dbo.Default_Accouts.Item_Code = dbo.tbl_Item_Master.Item_Code AND 
                         dbo.Default_Accouts.OPU_Code = dbo.tbl_Item_Master.BranchID
WHERE        (dbo.Default_Accouts.DefaultID IN (6, 8))]]></text>
    </command>
  </commands>
