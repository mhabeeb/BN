FROM         dbo.PR_Line_Items INNER JOIN
                      dbo.PR_Header ON dbo.PR_Line_Items.PR_NO = dbo.PR_Header.PR_NO INNER JOIN
                      dbo.tbl_BranchInfo ON dbo.PR_Header.OPU_Code = dbo.tbl_BranchInfo.BranchID]]></text>
