  <businessRules>
    <rule id="r100" commandName="New" type="Sql" phase="Execute"><![CDATA[SELECT @Project_Code=Project_Code,@Dept_Code=Dept_Code FROM PR_Header WHERE PR_NO=@PR_NO
SET @Created_By=@BusinessRules_UserId
SET @Created_DateTime=GETDATE()

SET @AProjectEnabled=@Session_ProjectEnabled

SET @ASiteCodeEnabled=@Session_SiteCodeEnabled
SET @ABudgetEnabled=@Session_BudgetEnabled
SET @BudgetCode=@Session_BudgetCode

]]></rule>
    <rule id="r101" commandName="Insert" type="Sql" phase="Before"><![CDATA[SET @PR_OPU_Code=@Session_OPU_Code
SET @IsBatchControl=IsNull(@IsBatchControl,0)
SET @IsItemSerialized=IsNull(@IsItemSerialized,0)
SET @P_Doc_No=[dbo].[GET_DOC_NO_FROM_EQU](@PR_NO  , 1)]]></rule>
    <rule id="r102" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    --SET @Result_ShowViewMessage='Sorry!, You Are ReadOnly User ....You Cannot Change Record'
	SET @Result_Focus = 'PR_Qty,You Are ReadOnly User ....You Cannot Change Record'
end
if [dbo].[CHECK_APPROVAL_STATUS](@PR_NO,1)=1
begin
    SET @BusinessRules_PreventDefault = 1
    --SET @Result_ShowViewMessage='Sorry!, The Docuement Is InProcess/Approved, Can not be Update'
	SET @Result_Focus = 'PR_Qty,The Docuement Is InProcess/Approved, Can not be Update'
end
]]></rule>
    <rule id="r103" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_Focus = 'PR_Qty, You Are WriteOnly User ....You Cannot Change Record'
end


if [dbo].[CHECK_DOCUMENT_STATUS](@PR_NO,5) IN (2,1)
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_Focus = 'PR_Qty, The Docuement Is InProcess, Can not be Update'
end]]></rule>
    <rule id="r104" type="Sql" commandName="Insert|Update" phase="Before" name="QT_NO_Validator"><![CDATA[if  dbo.CHECK_VALID_OPU_STORE (@BusinessRules_UserId,@Session_OPU_Code,@Session_StoreID) = 0
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'PR_Qty,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
end
IF IsNull(@P_Flag,0)=1
BEGIN
SET @BusinessRules_PreventDefault = 1
SET @Result_Focus = 'Item_Code,Workflow Has Been Started ';
END]]></rule>
    <rule id="r105" type="Sql" commandName="Calculate|Insert|Update" phase="Before" name="UOM_Converter"><![CDATA[if @P_UOM is null
    
begin
	SELECT @P_UOM = UOM FROM tbl_Item_Master WHERE Item_Code=@Item_Code
end

SET @UOM = IsNull(@UOM,@P_UOM)
SET @P_Qty = dbo.GET_CONVERTED_UOM_QTY(@Item_Code, @UOM, @PR_Qty);]]></rule>
