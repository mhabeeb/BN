<?xml version="1.0" encoding="utf-8"?>
<dataController name="Job_Order_Balance_Issuance" conflictDetection="overwriteChanges" label="^JobOrdBalIss^Job_Order_Balance_Issuance^JobOrdBalIss^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="dbo.DR_Lines" custom="true">
      <text><![CDATA[SELECT     dbo.DR_Lines.DR_ID, dbo.DR_Lines.DR_Line_ID, dbo.DR_Lines.SO_Line_No, dbo.DR_Lines.Item_Code, dbo.tbl_Item_Master.Part_No, 
                      dbo.tbl_Item_Master.Item_Description, dbo.tbl_Item_Master.UOM, dbo.DR_Lines.Job_ID, dbo.DR_Lines.Qty, dbo.DR_Lines.Issue_Balalnce, 
                      dbo.tbl_Item_Master.Stock_On_Hand, dbo.DR_Lines.DR_OPU_Code
FROM         dbo.DR_Lines INNER JOIN
                      dbo.tbl_Item_Master ON dbo.DR_Lines.Item_Code = dbo.tbl_Item_Master.Item_Code
WHERE     (dbo.DR_Lines.Issue_Balalnce > 0) AND (dbo.DR_Lines.SO_Line_No IS NULL)]]></text>
    </command>
  </commands>
  <fields>
    <field name="DR_ID" type="Decimal" allowNulls="false" label="^DrId^Dr#^DrId^" showInSummary="true" />
    <field name="DR_Line_ID" type="Decimal" allowNulls="false" isPrimaryKey="true" label="^DRLin^DR Line#^DRLin^" readOnly="true" />
    <field name="SO_Line_No" type="Decimal" label="^SOLinNo^SO Line No^SOLinNo^" showInSummary="true" />
    <field name="Item_Code" type="String" allowNulls="false" label="^IteCod^Item Code^IteCod^" length="20" showInSummary="true" />
    <field name="Part_No" type="String" label="^ParNo^Part No^ParNo^" length="20" showInSummary="true" />
    <field name="Item_Description" type="String" label="^IteDes^Item Description^IteDes^" length="500" showInSummary="true" />
    <field name="UOM" type="String" label="^Uom^Uom^Uom^" length="10" />
    <field name="Job_ID" type="Decimal" label="^JobId^Job#^JobId^" />
    <field name="Qty" type="Decimal" label="^Qty^Qty^Qty^" dataFormatString="N2" />
    <field name="Issue_Balalnce" type="Decimal" label="^IssBal^Issue Balalnce^IssBal^" readOnly="true" dataFormatString="N2" />
    <field name="Stock_On_Hand" type="Decimal" label="^StoOnHan^Stock On Hand^StoOnHan^" dataFormatString="N2" />
    <field name="DR_OPU_Code" type="Int32" label="^DROPUCod^DR OPU Code^DROPUCod^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" />
    <field name="ACompanyName" type="String" allowNulls="false" label="^AComNam^Company Name^AComNam^" allowQBE="false" allowSorting="false" computed="true" htmlEncode="false" hidden="false" length="100" readOnly="true" showInSummary="true">
      <formula>SELECT @Session_CompanyName</formula>
    </field>
    <field name="ABranch" type="String" allowNulls="false" label="^ABraNam^Branch Name^ABraNam^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" length="100" readOnly="true" computed="true" showInSummary="true">
