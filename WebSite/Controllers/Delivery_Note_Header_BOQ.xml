    <actionGroup id="ag100" scope="ActionBar" headerText="Full Invoice" flat="true">
      <action commandName="SQL" headerText="^FullInvoice^Full Invoice^FullInvoice^" id="a100" whenLastCommandName="Select" whenClientScript="[Rec_Status]==1">
        <data><![CDATA[EXEC [dbo].[MASS_RV_TO_AP_INVOICE] @Transaction_ID , @BusinessRules_UserId]]></data>
      </action>
      <action commandName="SQL" headerText="Ask Approval" id="a102" whenLastCommandName="Select" whenClientScript="[Approve_Ask]==0">
        <data><![CDATA[IF EXISTS
(
    SELECT TOP 1 Transaction_ID
    FROM tbl_Transactions_Line
    WHERE Transaction_ID = @Transaction_ID
)
    BEGIN
        EXEC XX_UPDATE_ASK_STATUS
             @Transaction_ID,
             @DOC_TYPE_ID,
             1,
             @BusinessRules_UserId
END]]></data>
      </action>
      <action commandName="SQL" headerText="Re-Open" id="a103" cssClass="glyphicon glyphicon-ok" whenLastCommandName="Select" whenClientScript="[Approve_Ask]==1">
        <data><![CDATA[EXECUTE [dbo].[REOPEN_DOCUMENT_STATUS] @Transaction_ID, @DOC_TYPE_ID]]></data>
      </action>
    </actionGroup>
    <actionGroup id="ag105" scope="ActionBar" headerText="Post GL" flat="true">
      <action commandName="SQL" headerText="Post Gl" id="a100" whenLastCommandName="Select" whenClientScript="[PostYN]==0 &amp;&amp; [Approve_Ask]==1">
