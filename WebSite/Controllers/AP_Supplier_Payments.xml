  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[SET @Rec_Status = 0
SET @PaymentType = 'Vendor Pay'
SET @APAR_Flag = 2
IF @Currency_Code IS NULL
    BEGIN
        SELECT @Currency_Code = IsNull(DefaultCurrency, 'SAR')
        FROM tbl_Customer_Supplier
        WHERE Sys_Number = @Sys_Number
END;


SET @Cur_Rate = [dbo].[Get_Curr_Rate_v1](@Currency_Code, @Payment_Date, @Session_OPU_Code)
If IsNull(@RefundYN,0)=1 and @Amount_Paid > 0
  BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Payment_Date,Refund Should Have -ve Amount'
    END


IF IsNull(@Cur_Rate , 0) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Payment_Date,Invalid Currency Rate ....Currency Rate Need to Define'
    END

SET @OPU_Code = @Session_OPU_Code
SET @Company_Code = @Session_Company_Code

SET @SUB_DOC_TYPE_ID=IsNull(@SUB_DOC_TYPE_ID,@DOC_TYPE_ID)

SET @Doc_No = dbo.GET_NEXT_DOC_NUMBER(@SUB_DOC_TYPE_ID, @Session_OPU_Code, @Payment_Date);
EXEC dbo.RESET_SEQUENCE
     @SUB_DOC_TYPE_ID,
     @Session_OPU_Code,
     @Payment_Date
	 
if IsNull(@Doc_No,0) < 0
begin
    SET @BusinessRules_PreventDefault = 1
 	SET @Result_Focus = 'Payment_Date,Invalid Document Sequence ....You Must Define The Sequence'
end

SET @CheckNo = [dbo].[GET_NEXT_DOC_NUMBER_BANK] (@PayNumber , @Session_OPU_Code , @Payment_Date)

EXEC [dbo].[RESET_SEQUENCE_BANK]
                          @PayNumber ,
                          @Session_OPU_Code ,
                          @Payment_Date
						  
SET @Clearing_Flag = dbo.GET_PAYMENT_DOC_CLEARYN (@PayNumber)

if IsNull(@CheckNo,0) < 0
begin
    SET @BusinessRules_PreventDefault = 1
 	SET @Result_Focus = 'Payment_Date,Invalid Payment Documents ....You Must Define The Sequence'
end]]></rule>
    <rule id="r102" commandName="New" type="Sql" phase="Execute"><![CDATA[SET @Payment_Date= getdate()
SET @Clearing_Flag=0
set @Currency_Code=@Session_DefaultCurrency

SET @Project_Code=@Session_DefProject_Code
--SET @SiteCode=@Session_DefSiteCode
SET @Dept_Code=@Session_DefDept_Code

SET @S1=@Session_DefS1
SET @S2=@Session_DefS2
SET @S3=@Session_DefS3
SET @S4=@Session_DefS4
SET @S5=@Session_DefS4

SET @GL_PostedYN=0
SET @Approve_Ask=0
SET @Rec_Status=0
SET @WireYN=0
SET @Int_OPU_Code=@Session_OPU_Code
SET @InterCompanyYN=0

SET @AProjectEnabled=@Session_ProjectEnabled
SET @ASiteCodeEnabled=@Session_SiteCodeEnabled

SET @DOC_TYPE_ID=1579


SELECT @Dimension1=Dimension1,@D1Caption=D1Caption,
@Dimension2=Dimension2,@D2Caption=D2Caption,
@Dimension3=Dimension3,@D3Caption=D3Caption,
@Dimension4=Dimension4,@D4Caption=D4Caption
FROM Accounting_Dimensions WHERE Company_Code=@Session_Company_Code

]]></rule>
    <rule id="r103" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[IF dbo.CHECK_VALID_OPU_STORE(@BusinessRules_UserId, @Session_OPU_Code, @Session_StoreID) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Payment_Date,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
END
if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Payment_Date, You Are ReadOnly User ....You Cannot Change Record'
end


if [dbo].[Check_Open_Date_SubLedger](@Payment_Date,@BusinessRules_UserId,1)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Payment_Date, The Transaction Date Is Not In An Open Period'
end


if [dbo].[CHECK_APPROVAL_STATUS](@PaymentID,@DOC_TYPE_ID)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Payment_Date, The Docuement Is InProcess/Approved, Can not be Update'
end
]]></rule>
    <rule id="r104" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
