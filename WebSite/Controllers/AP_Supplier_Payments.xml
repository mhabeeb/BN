  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[SET @Rec_Status = 0
SET @PaymentType = 'Vendor Pay'
SET @APAR_Flag = 2
IF @Currency_Code IS NULL
    BEGIN
        SELECT @Currency_Code = IsNull(DefaultCurrency, 'SAR')
        FROM tbl_Customer_Supplier
        WHERE Sys_Number = @Sys_Number
END;


SET @Cur_Rate = [dbo].[Get_Curr_Rate_v1](@Currency_Code, @Payment_Date, @Session_OPU_Code)
SET @OPU_Code = @Session_OPU_Code
SET @Company_Code = @Session_Company_Code


SET @Doc_No = dbo.GET_NEXT_DOC_NUMBER(@DOC_TYPE_ID, @Session_OPU_Code, @Payment_Date);
EXEC dbo.RESET_SEQUENCE
     @DOC_TYPE_ID,
     @Session_OPU_Code,
     @Payment_Date]]></rule>
    <rule id="r102" commandName="New" type="Sql" phase="Execute"><![CDATA[SET @Payment_Date= getdate()
SET @Clearing_Flag=0
set @Currency_Code='SAR'
SET @Project_Code='None'
SET @Dept_Code='None'
SET @GL_PostedYN=0
SET @Approve_Ask=0
SET @Rec_Status=0
SET @WireYN=0
SET @Int_OPU_Code=@Session_OPU_Code
SET @InterCompanyYN=0

SET @DOC_TYPE_ID=1579

--SET @Cur_Rate
]]></rule>
    <rule id="r103" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[IF dbo.CHECK_VALID_OPU_STORE(@BusinessRules_UserId, @Session_OPU_Code, @Session_StoreID) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Payment_Date,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
END
if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Payment_Date, You Are ReadOnly User ....You Cannot Change Record'
end


if [dbo].[Check_Open_Date_SubLedger](@Payment_Date,@BusinessRules_UserId,1)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Payment_Date, The Transaction Date Is Not In An Open Period'
end


if [dbo].[CHECK_APPROVAL_STATUS](@PaymentID,@DOC_TYPE_ID)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Payment_Date, The Docuement Is InProcess/Approved, Can not be Update'
end
]]></rule>
    <rule id="r104" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
