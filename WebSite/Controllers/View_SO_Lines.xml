<?xml version="1.0" encoding="utf-8"?>
<dataController name="View_SO_Lines" conflictDetection="overwriteChanges" label="^VieSoLin^View_SO_Lines^VieSoLin^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="SO_Line_Items" custom="true">
      <text><![CDATA[SELECT     SO_NO, SO_Line_No, Item_Code, SO_Qty, Unit_Price, Line_Remarks, QT_NO, DeliveryDate, Line_Net, PO_Balance, DV_Balance, INV_Balance, PR_Balance, 
                      SO_OPU_Code, Item_Desc, Item_Part, UOM, Project_Code, Dept_Code, Sys_Number, IsBatchControl, IsItemSerialized
FROM         dbo.SO_Line_Items]]></text>
    </command>
  </commands>
  <fields>
    <field name="SO_NO" type="Decimal" allowNulls="false" label="^SoNoBOQ^So No / BOQ^SoNoBOQ^" showInSummary="true" />
    <field name="SO_Line_No" type="Decimal" allowNulls="false" isPrimaryKey="true" label="^SOLinNo^SO Line No^SOLinNo^" readOnly="true" />
    <field name="Item_Code" type="String" allowNulls="false" label="^IteCod^Item Code^IteCod^" length="20" showInSummary="true" />
    <field name="SO_Qty" type="Decimal" label="^SOQty^SO Qty^SOQty^" showInSummary="true" />
    <field name="Unit_Price" type="Decimal" label="^UniPri^Unit Price^UniPri^" showInSummary="true" />
    <field name="Line_Remarks" type="String" label="^LinRem^Line Remarks^LinRem^" length="255" showInSummary="true" />
    <field name="QT_NO" type="Decimal" label="^QtNo^Qt No^QtNo^" />
    <field name="DeliveryDate" type="DateTime" label="^DelDat^Delivery Date^DelDat^" />
    <field name="Line_Net" type="Decimal" label="^LinNet^Line Net^LinNet^" readOnly="true" />
    <field name="PO_Balance" type="Decimal" label="^POBal^PO Balance^POBal^" />
    <field name="DV_Balance" type="Decimal" label="^DVBal^DV Balance^DVBal^" />
    <field name="INV_Balance" type="Decimal" label="^INVBal^INV Balance^INVBal^" />
    <field name="PR_Balance" type="Decimal" label="^PRBal^PR Balance^PRBal^" />
    <field name="SO_OPU_Code" type="Int32" label="^SoOPUCod^SO OPU Code^SoOPUCod^" />
    <field name="Item_Desc" type="String" label="^IteDes^Item Desc^IteDes^" length="500" />
    <field name="Item_Part" type="String" label="^ItePar^Item Part^ItePar^" length="20" />
    <field name="UOM" type="String" label="^Uom^Uom^Uom^" length="50" />
    <field name="Project_Code" type="String" label="^ProCod^Project Code^ProCod^" length="50" />
    <field name="Sys_Number" type="Decimal" label="^SysNum^Sys Number^SysNum^" />
    <field name="Dept_Code" type="String" label="^DepCod^Dept Code^DepCod^" length="50" />
    <field name="IsBatchControl" type="Int32" label="^IsBatCon^Is Batch Control^IsBatCon^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false">
      <items style="RadioButtonList" dataController="YesNo" dataView="grid1" dataTextField="Descriptions" dataValueField="ReturnValueInt" />
    </field>
    <field name="IsItemSerialized" type="Int32" label="^IsIteSer^Is Item Serialized^IsIteSer^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false">
      <items style="RadioButtonList" dataController="YesNo" dataView="grid1" dataTextField="Descriptions" dataValueField="ReturnValueInt" />
    </field>
    <field name="ACompanyName" type="String" allowNulls="false" label="^AComNam^Company Name^AComNam^" allowQBE="false" allowSorting="false" computed="true" htmlEncode="false" hidden="false" length="100" readOnly="true" showInSummary="true">
      <formula>SELECT @Session_CompanyName</formula>
    </field>
    <field name="ABranch" type="String" allowNulls="false" label="^BraNam^Branch Name^BraNam^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" length="100" readOnly="true" computed="true" showInSummary="true">
      <formula>SELECT @Session_BranchName </formula>
    </field>
    <field name="AStoreName" type="String" allowNulls="false" readOnly="true" label="^StoNam^Store Name^StorNam^" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" htmlEncode="false" hidden="false" length="100">
      <formula>SELECT  @Session_StoreName</formula>
    </field>
    <field name="ABranchAR" type="String" label="^ABraAr^ABranch AR^ABraAr^" computed="true" readOnly="true" showInSummary="true">
      <formula>SELECT @Session_BranchName_Ar</formula>
    </field>
    <field name="AddressEn" type="String" readOnly="true" label="^AddEn^Address En^AddEn^" computed="true" showInSummary="true">
      <formula>SELECT @Session_Address_English</formula>
    </field>
    <field name="AddressAr" type="String" readOnly="true" label="^AddAr^Address Ar^AddAr^" computed="true" showInSummary="true">
      <formula>SELECT @Session_Address_Arabic</formula>
    </field>
    <field name="ABranch_Code" type="Int32" label="^ABraCod^ABranch Code^ABraCod^" computed="true">
      <formula>SELECT @Session_OPU_Code</formula>
    </field>
    <field name="ABranchLogo" type="Byte[]" label="^ABraLog^ABranch Logo^ABraLog^" onDemand="true" onDemandHandler="tbl_BranchLogoOPULogo" onDemandStyle="Thumbnail" readOnly="true" allowQBE="false" showInSummary="true" sourceFields="ABranch_Code" />
  </fields>
