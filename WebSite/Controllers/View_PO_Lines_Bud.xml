    <command id="command1" type="Text" tableName="PO_Line_Items" custom="true">
      <text><![CDATA[SELECT        dbo.PO_Line_Items.PO_NO, dbo.PO_Line_Items.SO_NO, dbo.PO_Line_Items.Item_Code, dbo.PO_Line_Items.PR_NO, dbo.PO_Line_Items.PO_Qty, dbo.PO_Line_Items.Unit_Price, dbo.PO_Line_Items.Line_Total, 
                         dbo.PO_Line_Items.DeliveryDate, dbo.PO_Line_Items.CloseYN, dbo.PO_Line_Items.Line_Net, dbo.PO_Line_Items.RV_Balance, dbo.PO_Line_Items.Invoice_Balance, dbo.PO_Line_Items.Item_Desc, 
                         dbo.PO_Line_Items.Item_Part, dbo.PO_Line_Items.UOM, dbo.PO_Line_Items.PO_OPU_Code, dbo.PO_Line_Items.Project_Code, dbo.PO_Line_Items.Dept_Code, dbo.PO_Line_Items.SO_Line_No, 
                         dbo.PO_Line_Items.PO_Line_No, dbo.PO_Line_Items.IsBatchControl, dbo.PO_Line_Items.IsItemSerialized, dbo.PO_Line_Items.Item_Code_Ref_No, dbo.PO_Line_Items.BudgetAccount, dbo.PO_Line_Items.BudgetCode, 
                         dbo.PO_Line_Items.Created_DateTime, dbo.PO_Header.Rec_Status
FROM            dbo.PO_Line_Items INNER JOIN
                         dbo.PO_Header ON dbo.PO_Line_Items.PO_NO = dbo.PO_Header.PO_NO
WHERE        (dbo.PO_Header.Rec_Status = 1) AND (ISNULL(dbo.PO_Line_Items.RV_Balance, 0) + ISNULL(dbo.PO_Line_Items.Invoice_Balance, 0) > 0)]]></text>
    </command>
  </commands>
