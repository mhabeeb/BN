<?xml version="1.0" encoding="utf-8"?>
<dataController name="Auto_Expected_PO_Receipt" conflictDetection="overwriteChanges" label="^AutExpPoRec^Auto_Expected_PO_Receipt^AutExpPoRec^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="PO_Header" custom="true">
      <text><![CDATA[SELECT PO_Type,
       PO_Date,
       PO_NO,
       Supplier_QT_No,
       Sys_Number,
       Currency_Code,
       Vendor_Name,
       Header_Net,
       Project_Code,
       Doc_Status,
       Dept_Code,
       Company_Code,
       OPU_Code,
	   Approve_Ask,
	   Doc_No
FROM dbo.PO_Header
WHERE(PO_NO IN
     (
         SELECT PO_NO
         FROM dbo.PO_Line_Items
         WHERE(RV_Balance > 0)
     ))
	and Rec_Status=1]]></text>
    </command>
  </commands>
  <fields>
    <field name="PO_Type" type="String" label="^PoTyp^PO Type^PoTyp^" length="30" />
    <field name="PO_Date" type="DateTime" allowNulls="false" label="^PoDat^PO Date^PoDat^" />
    <field name="Doc_No" type="Int64" label="^DocNo^PONo^DocNo^" />
    <field name="PO_NO" type="Decimal" allowNulls="false" isPrimaryKey="true" label="^PoNo^PoNOSeq^PoNo^" readOnly="true" />
    <field name="Supplier_QT_No" type="String" label="^SupQtNo^Supplier QT No^SupQtNo^" length="15" />
    <field name="Sys_Number" type="Decimal" label="^SysNum^Supplier Number^SysNum^" />
    <field name="Currency_Code" type="String" label="^CurCod^Currency Code^CurCod^" length="4" />
    <field name="Vendor_Name" type="String" label="^VenNam^Vendor Name^VenNam^" readOnly="true" length="150" />
    <field name="Header_Net" type="Decimal" label="^HeaNet^Header Net^HeaNet^" readOnly="true" />
    <field name="Project_Code" type="String" label="^ProCod^Project Code^ProCod^" length="50" />
    <field name="Doc_Status" type="String" label="^DocSta^Doc Status^DocSta^" readOnly="true" length="15" />
    <field name="Dept_Code" type="String" label="^DepCod^Dept Code^DepCod^" length="50" />
    <field name="Company_Code" type="Int32" label="^ComCod^Company Code^ComCod^" />
    <field name="OPU_Code" type="Int64" label="^OpuCode^OPU Code^OpuCode^" />
    <field name="ACompanyName" type="String" allowNulls="false" label="^AComNam^Company Name^AComNam^" allowQBE="false" allowSorting="false" computed="true" htmlEncode="false" hidden="false" length="100" readOnly="true" showInSummary="true">
      <formula>SELECT @Session_CompanyName</formula>
    </field>
    <field name="ABranch" type="String" allowNulls="false" label="^ABra^Branch Name^ABra^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" length="100" readOnly="true" computed="true" showInSummary="true">
      <formula>SELECT @Session_BranchName </formula>
    </field>
    <field name="AStoreName" type="String" allowNulls="false" readOnly="true" label="^AStoNam^Store Name^AStoNam^" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" htmlEncode="false" hidden="false" length="100">
