  <businessRules>
    <rule id="r100" commandName="New" type="Sql" phase="Execute"><![CDATA[    SELECT 	@Inventory_AC=BR.Inventory_AC,
            @CostOfSales_AC=BR.CostOfSales_AC,
            @Revenue_AC=BR.Revenue_AC ,
            @Misc_Issue_AC=BR.Misc_Issue_AC,
            @Misc_Receipt_AC=BR.Misc_Receipt_AC,
			@Assets_Clearing_AC=BR.Assets_Clearing_AC,
			@Job_Issuance_Intransit_AC=BR.Job_Issuance_Intransit_AC,
			@Expence_AC=BR.Expence_AC FROM dbo.tbl_BranchInfo BR WHERE BR.BranchID=@Session_OPU_Code
			
			SET @IsBatchControl=0
			SET @AutoGenLotSerialYN=0
			SET @IsItemSerialized=0
			SET @IsServiceItem=0
			SET @Point_Of_SaleYN=0
			SET @BOQYN=0
			SET @IsAssets=0
			SET @ItemType='1'
			SET @autoItem=@Session_ItemCodeAutoYN
		IF @Session_ItemCodeAutoYN=1 SET @Item_Code='None'
		IF @Session_ItemCodeAutoYN=1 SET @Item_Code_Ref_No='None' 
			]]></rule>
    <rule id="r103" commandName="Insert" type="Sql" phase="Before"><![CDATA[    SELECT  @ItemSequence=IsNull(MAX(ItemSequence),0)
                 FROM [dbo].[tbl_Item_Master]
                 WHERE [Item_Category] = @Item_Category
                       AND [Item_Category_Major] = @Item_Category_Major
				   AND  BranchID=@Session_OPU_Code
SET @ItemSequence=IsNull(@ItemSequence,0)+1

SET @Secondry_UOM=@UOM
IF @Session_ItemCodeAutoYN=1 
BEGIN
    SET @Item_Code_Ref_No = [dbo].[GET_NEXT_ITEM_CODE](@Item_Category_Major, @Item_Category,@ItemSequence )
END
IF @Item_Code_Ref_No IS NULL SET @Item_Code_Ref_No= CAST(@ItemSequence AS NVARCHAR(20))
SET @Item_Code= CAST(@Session_OPU_Code AS NVARCHAR(5)) +'.' +@Item_Code_Ref_No 

]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[IF dbo.CHECK_VALID_OPU_STORE(@BusinessRules_UserId, @Session_OPU_Code, @Session_StoreID) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Re_Order_Qty,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
END
if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_Focus = 'Re_Order_Qty, You Are ReadOnly User ....You Cannot Change Record'
end
			SET @IsBatchControl=IsNull(@IsBatchControl,0)
			SET @AutoGenLotSerialYN=IsNull(@AutoGenLotSerialYN,0)
			SET @IsItemSerialized=IsNull(@IsItemSerialized,0)
			SET @IsServiceItem=IsNull(@IsServiceItem,0)
			SET @Point_Of_SaleYN=IsNull(@Point_Of_SaleYN,0)
			SET @BOQYN=IsNull(@BOQYN,0)
			SET @BranchID=@Session_OPU_Code
			
			SET @Company_Code=@Session_Company_Code
			
SET @Secondry_UOM=@UOM
]]></rule>
    <rule id="r102" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
    <rule id="r104" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
    <rule id="r105" commandName="Custom" commandArgument="ItmBarcode" type="JavaScript" phase="Execute"><![CDATA[window.open([Barcode], "_blank", "titlebar=no,location=no,toolbar=yes,scrollbars=no,resizable=yes,status=no,top=100,left=500,width=400,height=300");]]></rule>
    <rule id="r106" type="Sql" commandName="Insert|Update" phase="Before" name="IsBatchControl_Validator"><![CDATA[if IsNull(@IsBatchControl,0)=1 and IsNull(@IsItemSerialized,0)=1
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'IsBatchControl,Item Cannot be Set Both Batch Control & Serial Control It Should be Either'
end
]]></rule>
