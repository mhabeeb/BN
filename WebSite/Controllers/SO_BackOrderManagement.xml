<?xml version="1.0" encoding="utf-8"?>
<dataController name="SO_BackOrderManagement" conflictDetection="overwriteChanges" label="^SoBacOrdMan^SO_BackOrderManagement^SoBacOrdMan^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="dbo.SO_Line_Items" custom="true">
      <text><![CDATA[SELECT  dbo.SO_Line_Items.SO_NO, dbo.SO_Line_Items.SO_Line_No, dbo.SO_Line_Items.Item_Code, dbo.SO_Line_Items.SO_Qty, dbo.SO_Line_Items.Unit_Price, dbo.SO_Header.Order_Type, dbo.SO_Header.Sys_Number, dbo.SO_Header.Date_Ordered, 
                   dbo.SO_Header.Vendor_Name, dbo.SO_Line_Items.DV_Balance, dbo.SO_Line_Items.Item_Desc, dbo.SO_Line_Items.Item_Part, dbo.SO_Line_Items.UOM, ISNULL(dbo.SO_Line_Items.JOB, 0) AS JOB, ISNULL(dbo.SO_Line_Items.BO_PR, 0) AS BO_PR, 
                   ISNULL(dbo.SO_Line_Items.BO_PO, 0) AS BO_PO, dbo.SO_Header.BOQ, dbo.SO_Header.YourRefrence, dbo.SO_Header.OPU_Code, dbo.SO_Line_Items.Updated_By, dbo.SO_Line_Items.Updated_DateTime, dbo.SO_Line_Items.P_Doc_No, 
                   dbo.SO_Header.Doc_No, dbo.SO_Line_Items.Source_Doc_No, dbo.SO_Line_Items.Source_Doc_Type, dbo.SO_Line_Items.P_Qty, dbo.SO_Line_Items.P_UOM, dbo.SO_Header.Location_Name
FROM      dbo.SO_Line_Items INNER JOIN
                   dbo.SO_Header ON dbo.SO_Line_Items.SO_NO = dbo.SO_Header.SO_NO
WHERE   (ISNULL(dbo.SO_Line_Items.CloseYN, 0) = 0) AND (dbo.SO_Line_Items.DV_Balance > 0) AND (ISNULL(dbo.SO_Line_Items.JOB, 0) = 0) AND (ISNULL(dbo.SO_Line_Items.BO_PR, 0) = 0) AND (ISNULL(dbo.SO_Line_Items.BO_PO, 0) = 0) AND 
                   (ISNULL(dbo.SO_Header.Rec_Status, 0) = 1)]]></text>
    </command>
  </commands>
