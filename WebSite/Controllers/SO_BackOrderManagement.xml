    <field name="OPU_Code" type="Int32" label="^OPUCod^OPU Code^OPUCod^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" />
    <field name="Updated_By" type="Decimal" label="^UpdBy^Updated By^UpdBy^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" showInSummary="true" />
    <field name="Updated_DateTime" type="DateTime" label="^UpdDatTim^Updated Date Time^UpdDatTim^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" showInSummary="true" />
    <field name="ACompanyName" type="String" allowNulls="false" label="^CompNam^Company Name^CompNam^" allowQBE="false" allowSorting="false" computed="true" htmlEncode="false" hidden="false" length="100" readOnly="true" showInSummary="true">
