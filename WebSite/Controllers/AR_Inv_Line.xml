    <rule id="r103" type="Sql" commandName="Calculate|Insert|Update" phase="Before" name="Unit_Price_Converter"><![CDATA[IF IsNull(@Unit_Price_T, 0) > 0
      BEGIN
        SET @Line_Total_T = IsNull(@Qty, 0) * IsNull(@Unit_Price_T, 0)
        SET @Line_Net_T= [dbo].[GET_LINE_NET_CR_V1](@Line_Total_T, @Line_Disc_Percent, @Line_Disc_Amount_T, @Trx_ID)
END
]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
   -- SET @Result_ShowAlert=' You Are ReadOnly User ....You Cannot Change Record'
   SET @Result_Focus = 'Trx_ID,You Are ReadOnly User ....You Cannot Change Record'
end

declare @aError int

SET @aError=[dbo].[Cross_Validation_Accounts](@Distribution_AccountID,@Dept_Code,@Project_Code)

IF @aError <> 0 
BEGIN
    SET @BusinessRules_PreventDefault = 1
    IF @aError=1 SET @Result_Focus ='Item_Code, Project Code Required ....Cross Validation Rule Faild!'
    IF @aError=2 SET @Result_Focus ='Item_Code, Department Code Required ....Cross Validation Rule Faild!'
END


IF dbo.GET_INVOICE_TRX_STATUS(@Trx_ID) = 1
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Item_Code, Either Document Approved OR Posted to Gl Cannot Change or Add More'
    END
	
	IF [dbo].[Get_Accounts_require] ( @BusinessRules_UserId , 1 , @S1 ) = 1
    BEGIN
        SET @BusinessRules_PreventDefault = 1;
        SET @Result_Focus = 'S1,Value cannot be Default or Null';
    END;
	
	IF [dbo].[Get_Accounts_require] ( @BusinessRules_UserId , 2 , @S2 ) = 1
    BEGIN
        SET @BusinessRules_PreventDefault = 1;
        SET @Result_Focus = 'S2,Value cannot be Default or Null';
    END;
	
	IF [dbo].[Get_Accounts_require] ( @BusinessRules_UserId , 3 , @S3 ) = 1
    BEGIN
        SET @BusinessRules_PreventDefault = 1;
        SET @Result_Focus = 'S3,Value cannot be Default or Null';
    END;
	
	
	IF [dbo].[Get_Accounts_require] ( @BusinessRules_UserId , 4 , @S4 ) = 1
    BEGIN
        SET @BusinessRules_PreventDefault = 1;
        SET @Result_Focus = 'S4,Value cannot be Default or Null';
    END;
	
	IF [dbo].[Get_Accounts_require] ( @BusinessRules_UserId , 5, @S5 ) = 1
    BEGIN
        SET @BusinessRules_PreventDefault = 1;
        SET @Result_Focus = 'S5,Value cannot be Default or Null';
    END;	

]]></rule>
    <rule id="r100" commandName="New" type="Sql" phase="Execute"><![CDATA[SELECT @Project_Code=Project_Code ,
@Dept_Code=Dept_Code ,
@Currency_Code_T=Currency_Code FROM Credit_Trx_Header WHERE Trx_ID=@Trx_ID



SET @IsAssets=0
SET @Cr_OPU_Code=@Session_OPU_Code
SET @Int_Cr_OPU_Code=@Session_OPU_Code


SELECT @Dimension1=Dimension1,@D1Caption=D1Caption,
@Dimension2=Dimension2,@D2Caption=D2Caption,
@Dimension3=Dimension3,@D3Caption=D3Caption,
@Dimension4=Dimension4,@D4Caption=D4Caption,
@Dimension5=Dimension5,@D5Caption=D5Caption

FROM Accounting_Dimensions WHERE Company_Code=@Session_Company_Code

SET @BudgetCode=@Session_BudgetCode

SET @AProjectEnabled=@Session_ProjectEnabled
SET @ASiteCodeEnabled=@Session_SiteCodeEnabled
SET @ABudgetEnabled=@Session_BudgetEnabled

SET @Project_Code=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,-1)
SET @Dept_Code=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,0)

SET @S1=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,1)
SET @S2=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,2)
SET @S3=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,3)
SET @S4=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,4)
SET @S5=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,5)

]]></rule>
    <rule id="r104" type="Sql" commandName="Insert|Update" phase="Before" name="Item_Code_Validator"><![CDATA[IF EXISTS
(
    SELECT TOP 1 Trx_ID
    FROM Credit_Trx_Header
    WHERE Trx_ID = @Trx_ID
          AND (Rec_Status = 1
               OR PostYN = 1)
)
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Item_Code,Either Document Approved OR Posted to Gl Cannot Change or Add More'
end
]]></rule>
