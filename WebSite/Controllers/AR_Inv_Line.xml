  <businessRules>
    <rule id="r100" commandName="New" type="Sql" phase="Execute"><![CDATA[SELECT @Project_Code=Project_Code ,@Dept_Code=Dept_Code FROM Credit_Trx_Header WHERE Trx_ID=@Trx_ID

SET @IsAssets=0
SET @Cr_OPU_Code=@Session_OPU_Code

SET @Int_Cr_OPU_Code=@Session_OPU_Code


SELECT @Dimension1=Dimension1,@D1Caption=D1Caption,
@Dimension2=Dimension2,@D2Caption=D2Caption,
@Dimension3=Dimension3,@D3Caption=D3Caption,
@Dimension4=Dimension4,@D4Caption=D4Caption,
@Dimension5=Dimension5,@D5Caption=D5Caption,
@Dimension6=Dimension6,@D6Caption=D6Caption,
@Dimension7=Dimension7,@D7Caption=D7Caption,
@Dimension8=Dimension8,@D8Caption=D8Caption,
@Dimension9=Dimension9,@D9Caption=D9Caption

FROM Accounting_Dimensions WHERE Company_Code=@Session_Company_Code
]]></rule>
    <rule id="r103" type="Sql" commandName="Calculate|Insert|Update" phase="Before" name="Unit_Price_Converter"><![CDATA[IF IsNull(@Unit_Price, 0) > 0
   AND IsNull(@Qty, 0) > 0
    BEGIN
        SET @Line_Total = IsNull(@Qty, 0) * IsNull(@Unit_Price, 0)
        SET @Line_Net= [dbo].[GET_LINE_NET_CR_V1](@Line_Total, @Line_Disc_Percent, @Line_Disc_Amount, @Trx_ID)
END
]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
   -- SET @Result_ShowViewMessage='Sorry!, You Are ReadOnly User ....You Cannot Change Record'
   SET @Result_Focus = 'Trx_ID,You Are ReadOnly User ....You Cannot Change Record'
end

declare @aError int

SET @aError=[dbo].[Cross_Validation_Accounts](@Distribution_AccountID,@Dept_Code,@Project_Code)

IF @aError <> 0 
BEGIN
    SET @BusinessRules_PreventDefault = 1
    IF @aError=1 SET @Result_ShowViewMessage='Sorry!, Project Code Required ....Cross Validation Rule Faild!'
    IF @aError=2 SET @Result_ShowViewMessage='Sorry!, Department Code Required ....Cross Validation Rule Faild!'
END

if [dbo].[CHECK_APPROVAL_STATUS](@Trx_ID,6)=1
begin
    SET @BusinessRules_PreventDefault = 1
   -- SET @Result_ShowViewMessage='Sorry!, The Docuement Is InProcess/Approved, Can not be Update'
	SET @Result_Focus = 'Pick_Qty, The Docuement Is InProcess/Approved, Can not be Update'
end

]]></rule>
    <rule id="r102" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
    <rule id="r104" type="Sql" commandName="Insert|Update" phase="Before" name="Item_Code_Validator"><![CDATA[IF EXISTS
(
    SELECT TOP 1 Trx_ID
    FROM Credit_Trx_Header
    WHERE Trx_ID = @Trx_ID
          AND (Rec_Status = 1
               OR PostYN = 1)
)
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Item_Code,Either Document Approved OR Posted to Gl Cannot Change or Add More'
end
]]></rule>
    <rule id="r105" type="Sql" commandName="Update" phase="Before" name="Qty_Validator"><![CDATA[if @Source_SOPO_LineID > 0 
--and @Qty_NewValue <> @Qty_OldValue
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Qty,Cannot Edit Line.'
end
]]></rule>
  </businessRules>
