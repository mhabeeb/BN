  <businessRules>
    <rule id="r100" commandName="Update" type="Sql" phase="Before"><![CDATA[IF @Pick_Qty > 0 
BEGIN
UPDATE dbo.SO_Line_Items
SET Pick=1,
Pick_Qty=@Pick_Qty,
Pick_Doc=@Transaction_ID,
DR_Line_ID=@DR_Line_ID
