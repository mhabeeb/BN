  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[SET @Multiply=-1
--SET @Dr_AC=@Dr_AC,
--SET @Cr_AC=@Cr_AC,
SELECT  @Unit_Price=IsNull(Unit_Cost,0) FROM  tbl_Item_Master WHERE Item_Code=@Item_Code
SET @Qty=@QtyEnter * @Multiply 
SET @Transaction_No=1
SET @ReturnFlag='Job Issue'
SELECT  @Transaction_No=Transaction_No FROM  tbl_Transactions_Header WHERE Transaction_ID=@Transaction_ID


SET @Trx_OPU_Code=@Session_OPU_Code


DECLARE @On_hand_Qty money

SET @StoreID=@Session_StoreID
SET @On_hand_Qty=[dbo].[Get_Item_BAL_Store](@Item_Code,@StoreID)

IF IsNull(@Session_NegativeAllowedYN,0)=0 and @On_hand_Qty < @Qty and IsNull(@Multiply,0) < 0
begin
    set @BusinessRules_PreventDefault = 1
    set @Result_Focus = 'QtyEnter,Issuance Qty is Morethan Available Qty ::' + Cast(@On_hand_Qty as nvarchar(15))
end]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[SET @StoreID=@Session_StoreID
SET @On_hand_Qty=[dbo].[Get_Item_BAL_Store](@Item_Code,@StoreID)

IF dbo.CHECK_VALID_OPU_STORE(@BusinessRules_UserId, @Session_OPU_Code, @Session_StoreID) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Transaction_Date,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
END

IF IsNull(@Session_NegativeAllowedYN,0)=0 and @On_hand_Qty < @Qty and IsNull(@Multiply,0) < 0
begin
    set @BusinessRules_PreventDefault = 1
    set @Result_Focus = 'QtyEnter,Issuance Qty is Morethan Available Qty ::' + Cast(@On_hand_Qty as nvarchar(15))
end

if [dbo].[IsUserReadOnly](@BusinessRules_UserId)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are ReadOnly User ....You Cannot Change Record'
end
]]></rule>
    <rule id="r102" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if [dbo].[IsUserWriteOnly](@BusinessRules_UserId)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
  </businessRules>
