                      dbo.tbl_Transactions_Line.QtyEnter, dbo.tbl_Transactions_Line.Item_Serial_No, dbo.tbl_Transactions_Line.Line_Remarks, dbo.tbl_Transactions_Line.Unit_Price, 
                      dbo.tbl_Transactions_Line.Multiply, dbo.tbl_Transactions_Line.Project_Code, dbo.tbl_Transactions_Line.Dept_Code, dbo.tbl_Transactions_Line.UOM, 
                      dbo.tbl_Transactions_Line.Item_Part, dbo.tbl_Transactions_Line.Item_Desc, dbo.tbl_Transactions_Line.Line_Net, dbo.tbl_Transactions_Line.StoreID, 
                      dbo.tbl_Transactions_Line.Dr_AC, dbo.tbl_Transactions_Line.Cr_AC, dbo.tbl_Transactions_Line.Line_ID, 
                      dbo.tbl_Transactions_Header.Transaction_No AS TransactionNo, dbo.tbl_Transactions_Header.Project_Code AS ProjectCode, 
                      dbo.tbl_Transactions_Header.Dept_Code AS DeptCode, dbo.tbl_Transactions_Header.Vendor_Name, dbo.tbl_Transactions_Line.Unit_Cost, 
                      dbo.tbl_Transactions_Line.DR_Line_ID, dbo.tbl_Transactions_Line.ReturnFlag, dbo.tbl_Transactions_Line.Order_Price, dbo.tbl_Transactions_Line.Created_By, 
                      dbo.tbl_Transactions_Line.Created_DateTime, dbo.tbl_Transactions_Line.Updated_By, dbo.tbl_Transactions_Line.Updated_DateTime, 
                      dbo.tbl_Transactions_Line.Job_ID, dbo.tbl_Transactions_Line.Trx_OPU_Code
FROM         dbo.tbl_Transactions_Header INNER JOIN
