    <rule id="r100" commandName="New" type="Sql" phase="Execute"><![CDATA[SELECT @Project_Code=Project_Code ,@Dept_Code=Dept_Code,@Currency_Code_T=Currency_Code_T
FROM Credit_Trx_Header WHERE Trx_ID=@Trx_ID

SET @BudgetCode=@Session_BudgetCode
SET @IsAssets=0
SET @Int_Cr_OPU_Code=@Session_OPU_Code
SELECT @Cr_OPU_Code=IsNull(OPU_Code,1)  FROM dbo.Users
      WHERE UserID=@BusinessRules_UserId
  
SELECT @Dimension1=Dimension1,@D1Caption=D1Caption,
@Dimension2=Dimension2,@D2Caption=D2Caption,
@Dimension3=Dimension3,@D3Caption=D3Caption,
@Dimension4=Dimension4,@D4Caption=D4Caption,
@Dimension5=Dimension5,@D5Caption=D5Caption

FROM Accounting_Dimensions WHERE Company_Code=@Session_Company_Code

SET @AProjectEnabled=@Session_ProjectEnabled
SET @ASiteCodeEnabled=@Session_SiteCodeEnabled
SET @ABudgetEnabled=@Session_BudgetEnabled

SET @S1=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,1)
SET @S2=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,2)
SET @S3=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,3)
SET @S4=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,4)
SET @S5=dbo.Get_Accounts_DefaultValues (@BusinessRules_UserId,5)]]></rule>
    <rule id="r104" type="Sql" commandName="Insert|Update" phase="Before" name="Item_Code_Validator"><![CDATA[IF EXISTS
(
    SELECT TOP 1 Trx_ID
    FROM Credit_Trx_Header
    WHERE Trx_ID = @Trx_ID
          AND (Rec_Status = 1
               OR PostYN = 1)
)
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Item_Code,Either Document Approved OR Posted to Gl Cannot Change or Add More'
end
]]></rule>
    <rule id="r105" type="Sql" commandName="Update" phase="Before" name="Qty_Validator"><![CDATA[
