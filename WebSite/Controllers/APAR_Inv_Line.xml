--and @Qty_NewValue <> @Qty_OldValue
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Qty,Cannot Edit Line.'
end
]]></rule>
    <rule id="r107" commandName="Insert|Update" type="Sql" phase="Before"><![CDATA[DECLARE @Multiplier INT

SELECT  @Multiplier=Multiplier FROM Credit_Trx_Header WHERE Trx_ID=@Trx_ID

if @Qty > 0 and @Multiplier < 0
