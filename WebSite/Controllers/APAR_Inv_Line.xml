end]]></rule>
    <rule id="r103" type="Sql" commandName="Calculate|Insert|Update" phase="Before" name="Unit_Price_Converter"><![CDATA[IF IsNull(@Unit_Price_T, 0) > 0
  
    BEGIN
        SET @Line_Total_T = IsNull(@Qty, 0) * IsNull(@Unit_Price_T, 0)
        SET @Line_Net_T= [dbo].[GET_LINE_NET_CR_V1](@Line_Total_T, @Line_Disc_Percent, @Line_Disc_Amount_T, @Trx_ID)
END
]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[IF dbo.CHECK_VALID_OPU_STORE(@BusinessRules_UserId, @Session_OPU_Code, @Session_StoreID) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Qty,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
END
