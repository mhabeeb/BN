        <data><![CDATA[IF EXISTS
(
    SELECT TOP 1 Transaction_ID    FROM tbl_Transactions_Line
    WHERE Transaction_ID= @Transaction_ID
)       

begin
 EXEC XX_UPDATE_ASK_STATUS
             @Transaction_ID,
             @DOC_TYPE_ID,
             1,
             @BusinessRules_UserId
end]]></data>
