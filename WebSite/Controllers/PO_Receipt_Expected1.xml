  <businessRules>
    <rule id="r101" type="Sql" commandName="Update" phase="Before" name="Pick_Qty_Validator"><![CDATA[if @Pick_Qty > @RV_Balance
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Pick_Qty,Cannot be More then Pending Qty.'
end
	
If @IsBatchControl=1 AND @AutoGenLotSerialYN=0 and (@BatchExpiry is null OR @BatchNo is null)
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'BatchNo,Cannot be Empty BatchExpiry and BatchNo'
end
If @IsItemSerialized=1 AND @AutoGenLotSerialYN=0 and (@Item_Serial_No is null OR @Pick_Qty > 1)
begin
    /* prevent the default action processing */
    set @BusinessRules_PreventDefault = 1
    /* set the focus on the field and display an error */
    set @Result_Focus = 'Item_Serial_No,Cannot be Empty Serial No and Qty Should be 1'
end
/*If @IsItemSerialized=1 AND @AutoGenLotSerialYN=1 and  @Pick_Qty > 1
begin
       set @BusinessRules_PreventDefault = 1
    set @Result_Focus = 'Item_Serial_No,Qty Should be 1'
end*/
]]></rule>
    <rule id="r100" commandName="Update" type="Sql" phase="Before"><![CDATA[IF  @Pick_Qty > 0 
BEGIN
SET @Pick_Doc=@Transaction_ID 
SET @Pick=1
SET @Updated_By=@BusinessRules_UserId
SET @Updated_DateTime=GETDATE()
END]]></rule>
    <rule id="r102" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are ReadOnly User ....You Cannot Change Record'
end
]]></rule>
    <rule id="r103" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[if IsNull(@Session_IsWriteOnly,0)=1
