    <command id="command1" type="Text" tableName="PO_Line_Items" custom="true">
      <text><![CDATA[SELECT        dbo.PO_Line_Items.PO_NO, dbo.PO_Line_Items.SO_NO, dbo.PO_Line_Items.Item_Code, dbo.PO_Line_Items.PR_NO, dbo.PO_Line_Items.PO_Line_No, dbo.PO_Line_Items.PO_Qty, 
                         dbo.PO_Line_Items.Unit_Price, dbo.PO_Line_Items.Pick, dbo.PO_Line_Items.Pick_Qty, dbo.PO_Line_Items.Line_Total, dbo.PO_Line_Items.Pick_Doc, dbo.PO_Line_Items.Line_Remarks, 
                         dbo.PO_Line_Items.DeliveryDate, dbo.PO_Line_Items.Line_Net, dbo.PO_Line_Items.RV_Balance, dbo.PO_Line_Items.Invoice_Balance, dbo.PO_Line_Items.Item_Desc, dbo.PO_Line_Items.Item_Part, 
                         dbo.PO_Line_Items.UOM, dbo.PO_Line_Items.Dept_Code, dbo.PO_Line_Items.IsServiceItem, dbo.PO_Header.PO_Type, dbo.PO_Header.PO_Date, dbo.PO_Header.Supplier_QT_No, 
                         dbo.PO_Header.Currency_Code, dbo.PO_Header.Vendor_Name, dbo.PO_Header.ADDRESS_ALL, dbo.PO_Header.Doc_Status, dbo.PO_Line_Items.Sys_Number, dbo.PO_Line_Items.Project_Code, 
                         dbo.tbl_Transactions_Header.Transaction_ID, dbo.tbl_Transactions_Header.Transaction_Type, dbo.tbl_Transactions_Header.APAR_Flag, dbo.tbl_Transactions_Header.OPU_Code, 
                         dbo.PO_Line_Items.PO_OPU_Code, dbo.PO_Line_Items.Updated_By, dbo.PO_Line_Items.Updated_DateTime, dbo.PO_Line_Items.BatchNo, dbo.PO_Line_Items.BatchExpiry, dbo.PO_Line_Items.Item_Serial_No, 
                         dbo.PO_Line_Items.ShippingStoreID, dbo.PO_Line_Items.IsBatchControl, dbo.PO_Line_Items.IsItemSerialized, dbo.PO_Line_Items.Item_Code_Ref_No, dbo.PO_Header.Doc_No, dbo.PO_Header.Rec_Status, 
                         dbo.PO_Header.Approve_Ask, dbo.PO_Header.SiteCode, dbo.PO_Header.RFQ_NO
FROM            dbo.PO_Line_Items INNER JOIN
                         dbo.PO_Header ON dbo.PO_Line_Items.PO_NO = dbo.PO_Header.PO_NO INNER JOIN
                         dbo.tbl_Transactions_Header ON dbo.PO_Header.Sys_Number = dbo.tbl_Transactions_Header.Sys_Number AND dbo.PO_Header.OPU_Code = dbo.tbl_Transactions_Header.OPU_Code
WHERE        (dbo.PO_Line_Items.RV_Balance > 0) AND (dbo.tbl_Transactions_Header.Transaction_Type = N'Purchase Receipt') AND (dbo.tbl_Transactions_Header.APAR_Flag = 2)]]></text>
    </command>
