  <commands>
    <command id="command1" type="Text" tableName="Credit_Trx_Line_Items" custom="true">
      <text><![CDATA[SELECT        dbo.Credit_Trx_Line_Items.Item_Code, dbo.Credit_Trx_Line_Items.Qty, dbo.Credit_Trx_Line_Items.Unit_Price, dbo.Credit_Trx_Line_Items.Line_Total, dbo.Credit_Trx_Line_Items.Cr_OPU_Code, 
                         dbo.Credit_Trx_Line_Items.Transaction_ID, dbo.Credit_Trx_Line_Items.Line_Remarks, dbo.Credit_Trx_Line_Items.PO_NO, dbo.Credit_Trx_Line_Items.Trx_ID, dbo.Credit_Trx_Line_Items.SO_NO, 
                         dbo.Credit_Trx_Line_Items.Line_Net, dbo.Credit_Trx_Line_Items.Item_Desc, dbo.Credit_Trx_Line_Items.Item_Part, dbo.Credit_Trx_Line_Items.UOM, dbo.Credit_Trx_Line_Items.Project_Code, 
                         dbo.Credit_Trx_Line_Items.Dept_Code, dbo.Credit_Trx_Header.Invoice_Date, dbo.Credit_Trx_Header.Invoice_NO, dbo.Credit_Trx_Header.Vendor_Name, 
                         (CASE dbo.Credit_Trx_Header.APAR_Flag WHEN 1 THEN 'Customer Invoice' WHEN 2 THEN 'Supplier Invoice' END) AS InvoiceType, dbo.Credit_Trx_Line_Items.Source_SOPO_LineID AS PO_Line_No, 
                         dbo.Credit_Trx_Line_Items.Source_SOPO_LineID AS SO_Line_No, dbo.Credit_Trx_Line_Items.Item_Code_Ref_No
FROM            dbo.Credit_Trx_Line_Items INNER JOIN
                         dbo.Credit_Trx_Header ON dbo.Credit_Trx_Line_Items.Trx_ID = dbo.Credit_Trx_Header.Trx_ID]]></text>
    </command>
  </commands>
