                         dbo.tbl_Transactions_Line.Updated_DateTime, dbo.tbl_Transactions_Line.Multiply, dbo.tbl_Transactions_Line.Currency_Code, dbo.tbl_Transactions_Line.Dr_AC, dbo.tbl_Transactions_Line.Cr_AC, 
