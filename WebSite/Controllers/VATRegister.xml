<?xml version="1.0" encoding="utf-8"?>
<dataController name="VATRegister" conflictDetection="overwriteChanges" label="^VATReg^VATRegister^VATReg^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="GL_Transaction_Line" custom="true">
      <text><![CDATA[SELECT  dbo.GL_Transaction_Line.Transaction_Date, dbo.GL_Transaction_Line.AccountID, dbo.GL_Transaction_Line.Debit_Amount, dbo.GL_Transaction_Line.Credit_Amount, dbo.GL_Transaction_Line.Project_Code, dbo.GL_Transaction_Line.Dept_Code, 
                   dbo.GL_Transaction_Line.CalcDate, dbo.GL_Transaction_Line.PostType, dbo.Credit_Trx_Header.Vendor_Name AS Party_Name, dbo.Credit_Trx_Header.Doc_No, dbo.Credit_Trx_Header.OPU_Code, dbo.GL_Transaction_Line.GL_Line_No, 
                   dbo.GL_Transaction_Line.Trx_ID
FROM      dbo.GL_Transaction_Line INNER JOIN
                   dbo.Credit_Trx_Header ON dbo.GL_Transaction_Line.Trx_ID = dbo.Credit_Trx_Header.Trx_ID
WHERE   (dbo.GL_Transaction_Line.PostType = 50)]]></text>
    </command>
  </commands>
  <fields>
    <field name="Transaction_Date" type="DateTime" label="^TraDat^Transaction Date^TraDat^" showInSummary="true" />
    <field name="AccountID" type="String" allowNulls="false" label="^AccId^Account#^AccId^" length="50" showInSummary="true" />
    <field name="Debit_Amount" type="Decimal" allowNulls="false" label="^DebAmo^Debit Amount^DebAmo^" showInSummary="true" />
    <field name="Credit_Amount" type="Decimal" allowNulls="false" label="^CreAmo^Credit Amount^CreAmo^" showInSummary="true" />
    <field name="Project_Code" type="String" label="^ProCod^Project Code^ProCod^" length="50" showInSummary="true" />
    <field name="Dept_Code" type="String" label="^DepCod^Dept Code^DepCod^" length="50" />
    <field name="CalcDate" type="Int32" label="^CalDat^Calc Date^CalDat^" />
    <field name="PostType" type="Int32" label="^PosTyp^Post Type^PosTyp^" />
    <field name="Party_Name" type="String" label="^ParNam^Party Name^ParNam^" length="150" />
    <field name="Doc_No" type="Int64" label="^DocNo^Doc No^DocNo^" />
    <field name="OPU_Code" type="Int32" label="^OpuCod^OPU Code^OpuCod^" />
    <field name="GL_Line_No" type="Decimal" allowNulls="false" isPrimaryKey="true" label="^GlLinNo^GL Line No^GlLinNo^" readOnly="true" />
    <field name="Trx_ID" type="Decimal" label="^TrxId^Trx#^TrxId^" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="^TraLin^Transaction Line^TraLin^" filter="OPU_Code=@Session_OPU_Code">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="Trx_ID" columns="15" />
        <dataField fieldName="AccountID" columns="50" />
        <dataField fieldName="Transaction_Date" columns="10" />
        <dataField fieldName="Debit_Amount" dataFormatString="N2" columns="15" aggregate="Sum" />
        <dataField fieldName="Credit_Amount" dataFormatString="N2" columns="15" aggregate="Sum" />
        <dataField fieldName="Project_Code" columns="50" />
        <dataField fieldName="Dept_Code" columns="50" />
        <dataField fieldName="CalcDate" columns="15" />
        <dataField fieldName="PostType" columns="15" />
        <dataField fieldName="Party_Name" />
        <dataField fieldName="Doc_No" columns="15" />
        <dataField fieldName="OPU_Code" hidden="true" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review GL Transaction Line">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="GL Transaction Line" flow="NewColumn">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="Transaction_Date" columns="10" />
            <dataField fieldName="AccountID" columns="50" />
            <dataField fieldName="Debit_Amount" dataFormatString="c" columns="15" />
            <dataField fieldName="Credit_Amount" dataFormatString="c" columns="15" />
            <dataField fieldName="Project_Code" columns="50" />
            <dataField fieldName="Dept_Code" columns="50" />
            <dataField fieldName="CalcDate" columns="15" />
            <dataField fieldName="PostType" columns="15" />
            <dataField fieldName="Party_Name" />
            <dataField fieldName="Doc_No" columns="15" />
            <dataField fieldName="OPU_Code" hidden="true" columns="15" />
            <dataField fieldName="Trx_ID" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="^NewGlTraLin^New GL Transaction Line^NewGlTraLin^">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" headerText="New GL Transaction Line" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="Transaction_Date" columns="10" />
            <dataField fieldName="AccountID" columns="50" />
            <dataField fieldName="Debit_Amount" dataFormatString="c" columns="15" />
            <dataField fieldName="Credit_Amount" dataFormatString="c" columns="15" />
            <dataField fieldName="Project_Code" columns="50" />
            <dataField fieldName="Dept_Code" columns="50" />
            <dataField fieldName="CalcDate" columns="15" />
            <dataField fieldName="PostType" columns="15" />
            <dataField fieldName="Party_Name" />
            <dataField fieldName="Doc_No" columns="15" />
            <dataField fieldName="OPU_Code" hidden="true" columns="15" />
            <dataField fieldName="Trx_ID" columns="15" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
      <action id="a7" commandName="New" commandArgument="grid1" />
      <action id="a8" commandName="Duplicate" commandArgument="createForm1" />
      <action id="a9" />
      <action id="a10" commandName="BatchEdit" commandArgument="editForm1" />
    </actionGroup>
    <actionGroup id="ag2" scope="Form">
      <action id="a1" commandName="Edit" />
      <action id="a2" commandName="Delete" />
      <action id="a3" commandName="Cancel" />
      <action id="a4" whenLastCommandName="Edit" commandName="Update" />
      <action id="a5" whenLastCommandName="Edit" commandName="Delete" />
      <action id="a6" whenLastCommandName="Edit" commandName="Cancel" />
      <action id="a7" whenLastCommandName="New" commandName="Insert" />
      <action id="a8" whenLastCommandName="New" commandName="Cancel" />
      <action id="a9" whenLastCommandName="Duplicate" commandName="Insert" />
      <action id="a10" whenLastCommandName="Duplicate" commandName="Cancel" />
      <action id="a13" whenLastCommandName="Insert" whenView="createForm1" commandName="Select" commandArgument="editForm1" whenKeySelected="true" whenClientScript="this.hasDetails()" />
    </actionGroup>
    <actionGroup id="ag5" scope="ActionBar" headerText="Actions">
      <action id="a1" commandName="ExportCsv" />
      <action id="a2" />
      <action id="a3" commandName="ExportRowset" />
      <action id="a4" commandName="ExportRss" />
    </actionGroup>
    <actionGroup id="ag6" scope="ActionBar" headerText="Record">
      <action id="a1" whenLastCommandName="Edit" commandName="Update" />
      <action id="a2" whenLastCommandName="Edit" commandName="Cancel" />
      <action id="a4" whenLastCommandName="New" commandName="Cancel" />
    </actionGroup>
    <actionGroup id="ag7" scope="ActionBar" headerText="Report">
      <action id="a1" commandName="ReportAsPdf" />
      <action id="a2" commandName="ReportAsImage" />
      <action id="a3" commandName="ReportAsExcel" />
      <action id="a4" commandName="ReportAsWord" />
    </actionGroup>
    <actionGroup id="ag8" scope="Row">
      <action id="a4" whenLastCommandName="Edit" commandName="Update" />
      <action id="a5" whenLastCommandName="Edit" commandName="Cancel" />
      <action id="a7" whenLastCommandName="New" commandName="Cancel" />
    </actionGroup>
  </actions>
</dataController>