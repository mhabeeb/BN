      <action commandName="SQL" headerText="Ask Approval" id="a100" whenLastCommandName="Select" whenClientScript="[Approve_Ask]==0">
        <data><![CDATA[EXEC XX_UPDATE_ASK_STATUS @RFQ_NO,@DOC_TYPE_ID,1,@BusinessRules_UserId]]></data>
      </action>
      <action commandName="SQL" headerText="Re-Open" id="a101" cssClass="glyphicon glyphicon-ok" whenLastCommandName="Select" whenClientScript="[Approve_Ask]==1">
        <data><![CDATA[EXECUTE [dbo].[REOPEN_DOCUMENT_STATUS] @RFQ_NO,@DOC_TYPE_ID]]></data>
      </action>
