<dataController name="SOBOQ_Line_Items" label="^SoBoqLinIte^SOBOQ_Line_Items^SoBoqLinIte^" conflictDetection="overwriteChanges" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="dbo.SO_Line_Items" custom="true">
      <text><![CDATA[SELECT     dbo.SO_Line_Items.*, dbo.tbl_Contacts.ContactName, dbo.tbl_Contacts.ContactTitle, dbo.tbl_Contacts.eMailID, dbo.tbl_Contacts.Phone, dbo.SO_Header.Order_Type, 
                      dbo.SO_Header.Company_Code, dbo.SO_Header.OPU_Code, dbo.SO_Header.Date_Ordered, dbo.SO_Header.Salesman_Code, dbo.SO_Header.Customer_PO_No, 
                      dbo.SO_Header.Customer_PO_Date, dbo.SO_Header.Delivery_Date, dbo.SO_Header.Payment_Terms, dbo.SO_Header.Remarks, 
                      dbo.SO_Header.TermsANDConditions, dbo.SO_Header.HeaderDiscountYN, dbo.SO_Header.Header_Disc_Percent, dbo.SO_Header.Header_Disc_Amount, 
                      dbo.SO_Header.Approve, dbo.SO_Header.SalesMan_Name, dbo.SO_Header.ADDRESS_ALL, dbo.SO_Header.Vendor_Name, dbo.SO_Header.Header_Net, 
                      dbo.SO_Header.Header_Amount,  dbo.SO_Header.Doc_Status
FROM         dbo.SO_Header INNER JOIN
                      dbo.SO_Line_Items ON dbo.SO_Header.SO_NO = dbo.SO_Line_Items.SO_NO LEFT OUTER JOIN
                      dbo.tbl_Contacts ON dbo.SO_Header.ContactID = dbo.tbl_Contacts.ContactID]]></text>
    </command>
  </commands>
