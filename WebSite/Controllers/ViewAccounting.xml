      <text><![CDATA[SELECT        dbo.GL_Transaction_Header.GL_NO, dbo.GL_Transaction_Header.Transaction_Date, dbo.GL_Transaction_Line.Remark, dbo.GL_Transaction_Header.Tran_Type, dbo.GL_Transaction_Line.Debit_Amount, 
                         dbo.GL_Transaction_Line.Credit_Amount, dbo.GL_Transaction_Line.Dept_Code, dbo.GL_Transaction_Line.Ac_Description, dbo.GL_Transaction_Line.Project_Code, dbo.GL_Transaction_Line.Created_By, 
                         dbo.GL_Transaction_Line.Created_DateTime, dbo.GL_Transaction_Line.Line_NO, dbo.GL_Transaction_Line.TrxYear, dbo.GL_Transaction_Line.TrxMonths, dbo.GL_Transaction_Line.CalcDate, 
                         dbo.GL_Transaction_Line.Transaction_ID, dbo.GL_Transaction_Line.Trx_ID, dbo.GL_Transaction_Line.PaymentID, dbo.GL_Transaction_Line.Account_Type, dbo.GL_Transaction_Line.Lineage, dbo.GL_Transaction_Line.Line_ID, 
                         dbo.GL_Transaction_Line.AccountID, dbo.COA_Natural_Account.Ref_Account_No, dbo.GL_Transaction_Line.Item_Code, dbo.GL_Transaction_Line.AssetID, dbo.GL_Transaction_Line.ApplicationID, 
                         dbo.GL_Transaction_Header.GL_Serial_No, dbo.GL_Transaction_Line.OPU_Code_Ln, dbo.GL_Transaction_Line.S1, dbo.GL_Transaction_Line.S2, dbo.GL_Transaction_Line.S3, dbo.GL_Transaction_Line.S4, 
                         dbo.GL_Transaction_Line.S5, dbo.GL_Transaction_Line.S6, dbo.GL_Transaction_Line.S7, dbo.GL_Transaction_Line.S8, dbo.GL_Transaction_Line.S9, dbo.Accounting_Dimensions.Dimension1, 
                         dbo.Accounting_Dimensions.D1Caption, dbo.Accounting_Dimensions.Dimension2, dbo.Accounting_Dimensions.D2Caption, dbo.Accounting_Dimensions.Dimension3, dbo.Accounting_Dimensions.D3Caption, 
                         dbo.Accounting_Dimensions.Dimension4, dbo.Accounting_Dimensions.D4Caption, dbo.Accounting_Dimensions.Dimension5, dbo.Accounting_Dimensions.D5Caption, dbo.Accounting_Dimensions.Dimension6, 
                         dbo.Accounting_Dimensions.D6Caption, dbo.Accounting_Dimensions.Dimension7, dbo.Accounting_Dimensions.D7Caption, dbo.Accounting_Dimensions.Dimension8, dbo.Accounting_Dimensions.D8Caption, 
                         dbo.Accounting_Dimensions.Dimension9, dbo.Accounting_Dimensions.D9Caption, dbo.GL_Transaction_Line.GL_Line_No, dbo.GL_Transaction_Line.Company_Code_Ln, dbo.GL_Transaction_Header.Int_OPU_Code, 
                         dbo.GL_Transaction_Header.Trx_YYYYMM, dbo.GL_Transaction_Header.Batch_Name, dbo.GL_Transaction_Header.Int_Company_Code, dbo.GL_Transaction_Header.InterCompanyYN, dbo.GL_Transaction_Line.BudgetCode, 
                         dbo.GL_Transaction_Line.BudgetAccount,dbo.GL_Transaction_Header.OPU_Code,dbo.GL_Transaction_Line.Sys_Number, IsNUll(dbo.GL_Transaction_Line.PostType,0) AS PostType,dbo.GL_Transaction_Line.Store_ID,dbo.GL_Transaction_Line.CashBankID
FROM            dbo.GL_Transaction_Header INNER JOIN
                         dbo.GL_Transaction_Line ON dbo.GL_Transaction_Header.GL_Serial_No = dbo.GL_Transaction_Line.GL_Serial_No INNER JOIN
                         dbo.COA_Natural_Account ON dbo.GL_Transaction_Line.AccountID = dbo.COA_Natural_Account.AccountID INNER JOIN
                         dbo.Accounting_Dimensions ON dbo.GL_Transaction_Header.Company_Code = dbo.Accounting_Dimensions.Company_Code]]></text>
