                         dbo.tbl_Transactions_Header.Project_Code AS ProjectCode, dbo.tbl_Transactions_Header.Dept_Code AS DeptCode, dbo.tbl_Transactions_Header.Vendor_Name, dbo.tbl_Transactions_Line.Unit_Cost, 
