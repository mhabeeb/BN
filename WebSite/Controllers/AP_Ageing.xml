  <commands>
    <command id="command1" type="Text" tableName="  dbo.Credit_Trx_Header" custom="true">
      <text><![CDATA[SELECT     dbo.Credit_Trx_Header.Invoice_NO, dbo.Credit_Trx_Header.Invoice_Date, dbo.Credit_Trx_Header.Trx_ID, dbo.Credit_Trx_Header.Sys_Number, 
                      dbo.Credit_Trx_Header.Remarks, dbo.Credit_Trx_Header.Vendor_Name, dbo.Credit_Trx_Header.MonthAndYear, dbo.Credit_Trx_Header.Currency_Code, 
                      dbo.Credit_Trx_Header.Header_Amount, dbo.Credit_Trx_Header.Header_Net, dbo.Credit_Trx_Header.APPLY_BAL, 
                      dbo.ARAP_INV_AGE_4_BUCKET(dbo.Credit_Trx_Header.Invoice_Date) AS Ageing, dbo.Credit_Trx_Header.Doc_Date, dbo.Credit_Trx_Header.Doc_Number, 
                      dbo.V_Payment_Terms.Descriptions AS Payment, dbo.Credit_Trx_Header.OPU_Code, dbo.Credit_Trx_Header.Company_Code, dbo.Credit_Trx_Header.Project_Code, 
                      dbo.Credit_Trx_Header.PostYN, dbo.Credit_Trx_Header.Cur_Rate, dbo.Credit_Trx_Header.InvoiceTypes, dbo.Credit_Trx_Header.Ven_RefNo, 
                      dbo.Credit_Trx_Header.APPLY_BAL_BC, dbo.Credit_Trx_Header.Header_Net_BC,
					  dbo.Credit_Trx_Header.Dept_Code
FROM         dbo.Credit_Trx_Header LEFT OUTER JOIN
                      dbo.V_Payment_Terms ON dbo.Credit_Trx_Header.Payment_Terms = dbo.V_Payment_Terms.ReturnValueInt
WHERE     (dbo.Credit_Trx_Header.APAR_Flag = 2) AND (dbo.Credit_Trx_Header.APPLY_BAL <> 0)]]></text>
    </command>
  </commands>
