<?xml version="1.0" encoding="utf-8"?>
<dataController name="QuoteAnalysis" conflictDetection="overwriteChanges" label="^QuoAna^QuoteAnalysis^QuoAna^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="dbo.RFQ_Quotation" custom="true">
      <text><![CDATA[SELECT        dbo.RFQ_Quotation.RFQ_NO, dbo.RFQ_Quotation.Item_Code, dbo.RFQ_Quotation.UOM, dbo.RFQ_Quotation.RFQ_Qty, dbo.RFQ_Quotation.Unit_Price, 
                         dbo.RFQ_Quotation.Notes, dbo.RFQ_Quotation.Terms, dbo.RFQ_Quotation.Vendor_Name, dbo.RFQ_Quotation.Promised_Date, dbo.RFQ_Quotation.ConvertToPO, 
                         dbo.tbl_Item_Master.Part_No, dbo.tbl_Item_Master.Item_Description, dbo.RFQ_Header.RFQ_Date, dbo.RFQ_Header.Last_Submission_Date, 
                         dbo.RFQ_Header.RFQ_Status, dbo.RFQ_Header.OPU_Code, dbo.RFQ_Header.Company_Code,dbo.RFQ_Header.Approve_Ask,dbo.RFQ_Quotation.PR_Line_No,dbo.RFQ_Quotation.Sys_Number,RFQ_Quotation.ClosedYN
FROM            dbo.RFQ_Quotation INNER JOIN
                         dbo.tbl_Item_Master ON dbo.RFQ_Quotation.Item_Code = dbo.tbl_Item_Master.Item_Code INNER JOIN
                         dbo.RFQ_Header ON dbo.RFQ_Quotation.RFQ_NO = dbo.RFQ_Header.RFQ_NO and dbo.RFQ_Header.Approve_Ask=1]]></text>
    </command>
  </commands>
  <fields>
    <field name="ACompanyName" type="String" allowNulls="false" label="^AComNam^Company Name^AComNam^" allowQBE="false" allowSorting="false" computed="true" htmlEncode="false" hidden="false" length="100" readOnly="true" showInSummary="true">
      <formula>SELECT @Session_CompanyName</formula>
    </field>
    <field name="ABranch" type="String" allowNulls="false" label="^ABra^Branch Name^ABra^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" length="100" readOnly="true" computed="true" showInSummary="true">
      <formula>SELECT @Session_BranchName </formula>
    </field>
    <field name="AStoreName" type="String" allowNulls="false" readOnly="true" label="^AStoNam^Store Name^AStoNam^" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" htmlEncode="false" hidden="false" length="100">
      <formula>SELECT  @Session_StoreName</formula>
    </field>
    <field name="ABranchAR" type="String" label="^ABraAr^ABranch AR^ABraAr^" computed="true" readOnly="true">
      <formula>SELECT @Session_BranchName_Ar</formula>
    </field>
    <field name="AddressEn" type="String" readOnly="true" label="^AddEn^Address En^AddEn^" computed="true">
      <formula>SELECT @Session_Address_English</formula>
    </field>
    <field name="AddressAr" type="String" readOnly="true" label="^AddAr^Address Ar^AddAr^" computed="true">
      <formula>SELECT @Session_Address_Arabic</formula>
    </field>
    <field name="RFQ_NO" type="Decimal" allowNulls="false" label="^RfqNo^Rfq No^RfqNo^" showInSummary="true" isPrimaryKey="true" />
    <field name="Item_Code" type="String" label="^IteCod^Item Code^IteCod^" length="20" />
    <field name="UOM" type="String" label="^Uom^Uom^Uom^" readOnly="true" length="10" />
    <field name="RFQ_Qty" type="Decimal" label="^RfqQty^RFQ Qty^RfqQty^" showInSummary="true" />
    <field name="Unit_Price" type="Decimal" label="^UniPri^Unit Price^UniPri^" />
    <field name="Notes" type="String" allowQBE="false" allowSorting="false" label="^Notes^Notes^Notes^" />
    <field name="Terms" type="String" allowQBE="false" allowSorting="false" label="^Terms^Terms^Terms^" />
    <field name="Sys_Number" type="Decimal" allowNulls="false" label="^SysNum^SupplierNo^SysNum^" />
    <field name="Vendor_Name" type="String" label="^VenNam^Vendor Name^VenNam^" length="150" />
    <field name="Promised_Date" type="DateTime" label="^ProDat^Promised Date^ProDat^" />
    <field name="ConvertToPO" type="Int32" label="^ConToPo^Convert To PO^ConToPo^" />
    <field name="Part_No" type="String" label="^ParNo^Part No^ParNo^" length="20" />
    <field name="Item_Description" type="String" label="^IteDes^Item Description^IteDes^" length="500" />
    <field name="RFQ_Date" type="DateTime" label="^RfqDat^RFQ Date^RfqDat^" />
    <field name="Last_Submission_Date" type="DateTime" label="^LasSubDat^Last Submission Date^LasSubDat^" />
    <field name="RFQ_Status" type="String" label="^RfqSta^RFQ Status^RfqSta^" length="25" />
    <field name="OPU_Code" type="Int32" label="^OpuCod^OPU Code^OpuCod^" />
    <field name="Company_Code" type="Int32" label="^ComCod^Company Code^ComCod^" />
    <field name="PR_Line_No" type="Decimal" allowNulls="false" label="^PrLinNo^PR Line No^PrLinNo^" />
    <field name="ClosedYN" type="Int32" label="^CloYn^Closed YN^CloYn^">
      <items style="RadioButtonList" dataController="YesNo" dataView="grid1" dataTextField="Descriptions" dataValueField="ReturnValueInt" />
    </field>
    <field name="ABranchLogo" type="Byte[]" label="^ABraLog^ABranch Logo^ABraLog^" computed="true" onDemand="true" onDemandHandler="AABranch_LogoBLogo12" onDemandStyle="Thumbnail" readOnly="true" allowQBE="false" sourceFields="OPU_Code" isVirtual="true" />
  </fields>
