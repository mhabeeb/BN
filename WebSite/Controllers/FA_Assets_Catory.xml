    </field>
    <field name="Assets_Accum_Depr_Account" type="String" label="^AssAccDepAc^Assets Accum Depr. A/C^AssAccDepAc^" length="50" allowNulls="false">
      <items style="Lookup" dataController="Pick_COA_Child" dataView="grid1" dataTextField="Account_Description" dataValueField="AccountID" />
    </field>
    <field name="Created_By" type="Decimal" label="^CreBy^Created By^CreBy^" showInSummary="true">
      <items style="DropDownList" dataController="Pick_Users" dataView="grid1" dataTextField="UserName" dataValueField="UserID" />
    </field>
    <field name="Created_DateTime" type="DateTime" label="^CreDatTim^Created Date Time^CreDatTim^" showInSummary="true" />
    <field name="Updated_By" type="Double" label="^UpdBy^Updated By^UpdBy^" showInSummary="true">
      <items style="DropDownList" dataController="Pick_Users" dataView="grid1" dataTextField="UserName" dataValueField="UserID" />
    </field>
    <field name="Updated_DateTime" type="DateTime" label="^UpdDatTim^Updated Date Time^UpdDatTim^" showInSummary="true" />
