    </field>
    <field name="AssetCategory" type="String" label="^AssCat^Asset Category^AssCat^" length="50" allowNulls="false" />
    <field name="Assets_Cost_Account" type="String" label="^AssCosAc^Assets Cost A/C^AssCosAc^" length="50" allowNulls="false">
      <items style="Lookup" dataController="Pick_COA_Child" dataView="grid1" dataTextField="Account_Description" dataValueField="AccountID" />
    </field>
    <field name="Assets_Cost_Clearing_AC" type="String" label="^AssCosCleAc^Assets Cost Clearing A/C^AssCosCleAc^" length="50" allowNulls="false">
