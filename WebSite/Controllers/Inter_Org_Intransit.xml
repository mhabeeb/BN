<?xml version="1.0" encoding="utf-8"?>
<dataController name="Inter_Org_Intransit" conflictDetection="overwriteChanges" label="^IntOrgInt^Inter_Org_Intransit^IntOrgInt^" plugIn="MyCompany.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text" tableName="tbl_Transactions_Header" custom="true">
      <text><![CDATA[SELECT        dbo.tbl_Store.Store_Name, dbo.tbl_Store.Store_Address, dbo.tbl_Store.OPU_Code, dbo.tbl_Store.Store_ID, dbo.tbl_Transactions_Header.Transaction_ID, 
                         dbo.tbl_Transactions_Header.Remarks, ISNULL(dbo.tbl_Transactions_Header.IsInterOrg, 0) AS IsInterOrg, dbo.tbl_Transactions_Header.To_StoreID, 
                         ISNULL(dbo.tbl_Transactions_Header.ImportYN, 0) AS ImportYN, dbo.tbl_Transactions_Header.ImportDate, dbo.tbl_Transactions_Header.Transaction_Date, 
                         dbo.tbl_Transactions_Header.Transaction_No
FROM            dbo.tbl_Store INNER JOIN
                         dbo.tbl_Transactions_Header ON dbo.tbl_Store.Store_ID = dbo.tbl_Transactions_Header.StoreID
WHERE        (ISNULL(dbo.tbl_Transactions_Header.IsInterOrg, 0) = 1) AND (ISNULL(dbo.tbl_Transactions_Header.ImportYN, 0) = 0)]]></text>
    </command>
  </commands>
  <fields>
    <field name="Store_Name" type="String" label="^StorNam^Store Name^StorNam^" length="255" showInSummary="true" />
    <field name="Store_Address" type="String" label="^StoAdd^Store Address^StoAdd^" length="255" showInSummary="true" />
    <field name="OPU_Code" type="Int32" label="^OpuCod^OPU Code^OpuCod^" showInSummary="true" />
    <field name="Store_ID" type="Int32" allowNulls="false" label="^StoId^Store#^StoId^" showInSummary="true" readOnly="true" />
    <field name="Transaction_ID" type="Decimal" allowNulls="false" isPrimaryKey="true" label="^TraId^Transaction#^TraId^" readOnly="true" showInSummary="true" />
    <field name="Remarks" type="String" label="^Rem^Remarks^Rem^" length="50" showInSummary="true" />
    <field name="IsInterOrg" type="Int32" allowNulls="false" label="^IsIntOrg^Is Inter Org^IsIntOrg^" readOnly="true" />
    <field name="To_StoreID" type="Int32" label="^ToStoId^To Store#^ToStoId^" />
    <field name="ImportYN" type="Int32" allowNulls="false" label="^ImpYn^Import YN^ImpYn^" readOnly="true" />
    <field name="ImportDate" type="DateTime" label="^ImpDat^Import Date^ImpDat^" />
    <field name="Transaction_Date" type="DateTime" label="^TraDat^Transaction Date^TraDat^" />
    <field name="Transaction_No" type="Decimal" label="^TraNo^Ref No^TraNo^" />
    <field name="ACompanyName" type="String" allowNulls="false" label="^AComNam^CompanyName^AComNam^" allowQBE="false" allowSorting="false" computed="true" htmlEncode="false" hidden="false" length="100" readOnly="true" showInSummary="true">
      <formula>SELECT @Session_CompanyName</formula>
    </field>
    <field name="ABranch" type="String" allowNulls="false" label="^ABra^Branch Name^ABra^" allowQBE="false" allowSorting="false" htmlEncode="false" hidden="false" length="100" readOnly="true" computed="true" showInSummary="true">
      <formula>SELECT @Session_BranchName </formula>
    </field>
    <field name="AStoreName" type="String" allowNulls="false" readOnly="true" label="^AStoNam^Store Name^AStoNam^" allowQBE="false" allowSorting="false" computed="true" showInSummary="true" htmlEncode="false" hidden="false" length="100">
