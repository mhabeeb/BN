  <businessRules>
    <rule id="r100" commandName="Insert" type="Sql" phase="Before"><![CDATA[SET @APAR_Flag=2

IF @Currency_Code IS NULL
BEGIN
	SELECT @Currency_Code=IsNull(DefaultCurrency,@Session_DefaultCurrency)  from tbl_Customer_Supplier 
	WHERE Sys_Number=@Sys_Number
END

SET @Cur_Rate = [dbo].[Get_Curr_Rate_v1] (@Currency_Code,@Invoice_Date, @Session_OPU_Code)

SELECT @Invoice_NO = IsNull(MAX(Invoice_NO), 0)
FROM dbo.Credit_Trx_Header
WHERE APAR_Flag = 2
      AND InvoiceTypes = @InvoiceTypes
IF EXISTS
(
    SELECT TOP 1 InvoiceTypes
    FROM dbo.tbl_Invoice_Types
    WHERE InvoiceTypes = @InvoiceTypes
          AND StartYN = 1
)
    BEGIN
        SELECT @Invoice_NO = IsNull(StartInvoiceNumber, 0)
        FROM dbo.tbl_Invoice_Types
        WHERE APAR_Flag = 2
              AND InvoiceTypes = @InvoiceTypes
        UPDATE tbl_Invoice_Types
          SET
              StartYN = 0
        WHERE InvoiceTypes = @InvoiceTypes
              AND APAR_Flag = 2
END
SET @Invoice_NO = @Invoice_NO + 1

	  
	  
SET @OPU_Code=@Session_OPU_Code
SET @Company_Code=@Session_Company_Code

SET @Rec_Status=0


--SET @Doc_No = dbo.GET_NEXT_DOC_NUMBER (5,@Session_OPU_Code,@Invoice_Date)

SET @Doc_No = dbo.GET_NEXT_DOC_NUMBER (@DOC_TYPE_ID,@Session_OPU_Code,@Invoice_Date)
EXEC dbo.RESET_SEQUENCE @DOC_TYPE_ID,@Session_OPU_Code,@Invoice_Date
	  

]]></rule>
    <rule id="r102" commandName="New" type="Sql" phase="Execute"><![CDATA[SET @Invoice_Date= GETDATE()
SET @Doc_Date=GETDATE()
SET @Project_Code='None'
SET @Dept_Code='None'
SET @RetentionYN=0
SET @Retention_Percentage=0
SET @Retention_Amount=0
SET @Approve_Ask=0
SET @Rec_Status=0
SET @PostYN=0
Set @DOC_TYPE_ID=5



]]></rule>
    <rule id="r101" commandName="Insert|Update|Delete" type="Sql" phase="Before"><![CDATA[IF dbo.CHECK_VALID_OPU_STORE(@BusinessRules_UserId, @Session_OPU_Code, @Session_StoreID) = 0
    BEGIN
        SET @BusinessRules_PreventDefault = 1
        SET @Result_Focus = 'Invoice_Date,Invalid Store/Branch Selection Please select valid Organization from Change Organization Page.'
END

if IsNull(@Session_IsReadOnly,0)=1
begin
    SET @BusinessRules_PreventDefault = 1
	SET @Result_Focus = 'Invoice_Date,You Are ReadOnly User ....You Cannot Change Record'
end


if [dbo].[CHECK_APPROVAL_STATUS](@Trx_ID,@DOC_TYPE_ID)=1
begin
    SET @BusinessRules_PreventDefault = 1
    --SET @Result_ShowViewMessage='Sorry!, The Docuement Is InProcess/Approved, Can not be Update'
	SET @Result_Focus = 'Invoice_Date,The Docuement Is InProcess/Approved, Can not be Update'
end

DECLARE @N_DAYS INT

SELECT  @N_DAYS=IsNull(No_Days,0) FROM LookUp_PaymentTerms
WHERE Pay_Terms_ID=@Payment_Terms

SET @N_DAYS=IsNull(@N_DAYS,0)

SET @Due_Date=DATEADD(day,@N_DAYS,@Invoice_Date)

IF IsNull(@Retention_Percentage,0) + IsNull(@Retention_Amount,0) > 0  SET @RetentionYN=1

if [dbo].[Check_Open_Date_SubLedger](@Invoice_Date,@BusinessRules_UserId,1)=1
begin
    SET @BusinessRules_PreventDefault = 1
    --SET @Result_ShowViewMessage='Sorry!, The Transaction Date Is Not In An Open Period'
	SET @Result_Focus = 'Invoice_Date,The Transaction Date Is Not In An Open Period'
end]]></rule>
    <rule id="r103" commandName="Update|Delete" type="Sql" phase="Before"><![CDATA[SET @Updated_By = @BusinessRules_UserId

if IsNull(@Session_IsWriteOnly,0)=1

begin
    SET @BusinessRules_PreventDefault = 1
    SET @Result_ShowViewMessage='Sorry!, You Are WriteOnly User ....You Cannot Change Record'
end]]></rule>
  </businessRules>
